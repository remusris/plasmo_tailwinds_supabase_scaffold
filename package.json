{
  "name": "plasmo-tailwinds-supabase-scaffold",
  "displayName": "Plasmo tailwinds supabase scaffold",
  "version": "0.0.1",
  "description": "scaffold for supabase auth, tailwinds using plasmo ",
  "author": "Remus Risnoveanu",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "test": "plasmo test"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.2.0",
    "@plasmohq/messaging": "^0.5.0",
    "@plasmohq/storage": "^1.7.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.4",
    "@supabase/supabase-js": "^2.32.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.265.0",
    "plasmo": "0.82.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@types/chrome": "0.0.241",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.27",
    "prettier": "3.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "options.html"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ]
  }
}
